{
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "paths": {
      "@typechain/*": ["./typechain/*"],
      "@abis/*": ["../abis/*"],
      "@ajna-contracts/*": ["../ajna-contracts/*"],
      "@ajna-contracts/scripts": ["../ajna-contracts/scripts/index.ts"],
      "@ajna-contracts/scripts/*": ["../ajna-contracts/scripts/*"],
      "@dma-contracts/test/*": ["./test/*"],
      "@dma-contracts/scripts/*": ["./scripts/*"],
      "@dma-contracts/*": ["./*"],
      "@dma-library": ["../dma-library/src/index.ts"],
      "@dma-library/*": ["../dma-library/src/*"],
      "@dma-common/constants": ["../dma-common/constants/index.ts"],
      "@dma-common/utils/*": ["../dma-common/utils/*"],
      "@dma-common/types/*": ["../dma-common/types/*"],
      "@dma-common/test-utils": ["../dma-common/test-utils/index.ts"],
      "@dma-common/test-utils/*": ["../dma-common/test-utils/*"],
      "@deploy-configurations/configs": ["../deploy-configurations/configs"],
      "@deploy-configurations/addresses": ["../deploy-configurations/addresses/index.ts"],
      "@deploy-configurations/utils/*": ["../deploy-configurations/utils/*"],
      "@deploy-configurations/types/*": ["../deploy-configurations/types/*"],
      "@deploy-configurations/deployment/*": ["../deploy-configurations/deployment/*"],
      "@deploy-configurations/constants": ["../deploy-configurations/constants/index.ts"],
      "@deploy-configurations/operation-definitions": [
        "../deploy-configurations/operation-definitions/index.ts"
      ],
      "@domain/*": ["../domain/src/*"],
      "@domain": ["../domain/src/index.ts"]
    },
    "typeRoots": ["../../node_modules/@types"]
  },
  "references": [
    {
      "path": "../abis"
    },
    {
      "path": "../domain"
    },
    {
      "path": "../dma-common"
    },
    {
      "path": "../dma-library"
    },
    {
      "path": "../deploy-configurations"
    },
    {
      "path": "../ajna-contracts"
    }
  ],
  "include": ["./test", "./tasks", "./scripts", "./bootstrap-env.ts", "./hardhat.config.ts"]
}
