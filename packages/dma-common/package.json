    "./types/": "./types/",
    "./utils/*": "./utils/*/index.ts"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "bignumber.js": "9.0.1",
    "ethers": "5.7.2",
    "ts-try": "^0.2.0"
  },
  "devDependencies": {
    "@types/sinon-chai": "^3.2.9"
  },
  "files": [
    "/test-utils",
    "/utils",
    "/constants"
  ],
  "nx": {
    "implicitDependencies": ["@oasisdex/dma-deployments"]
  }
