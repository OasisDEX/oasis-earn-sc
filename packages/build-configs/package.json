{
  "name": "build-configs",
  "version": "0.0.1",
  "description": "Build configs holder.",
  "author": "Marcin Ciarka <marcin@oazoapps.com>",
  "homepage": "https://github.com/OasisDEX/oasis-earn-sc#readme",
  "license": "ISC",
  "devDependencies": {
    "parcel": "2.8.3"
  },
  "scripts": {
    "build": "yarn build:addresses && yarn build:dma-library",
    "build:dma-library": "yarn --cwd ./dma-library build:dma-library",
    "build:addresses": "yarn --cwd ./addresses build:addresses",
    "link:all": "yarn link:dma-library && yarn link:addresses",
    "unlink:all": "yarn unlink:dma-library && yarn unlink:addresses",
    "link:dma-library": "yarn --cwd ./dma-library link",
    "unlink:dma-library": "yarn --cwd ./dma-library unlink",
    "link:addresses": "yarn --cwd ./addresses link",
    "unlink:addresses": "yarn --cwd ./addresses unlink"
  }
}
