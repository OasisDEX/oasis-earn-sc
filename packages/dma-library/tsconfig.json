{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "strict": true,
    "outDir": "lib",
    "baseUrl": ".",
    "paths": {
      "@typechain/*": [
        "../dma-contracts/typechain/*"
      ],
      "@dma-library/*": [
        "./src/*"
      ],
      "@dma-library": [
        "./src/index.ts"
      ],
      "utils/*": ["../dma-common/utils/*"],
      "@oasisdex/addresses": ["../addresses/src/index.ts"],
      "@oasisdex/domain/*": ["../domain/src/*"],
      "@oasisdex/domain": ["../domain/src/index.ts"]
    },
    "types": [
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "include": [
    "./hardhat.config.ts",
    "./src/**/*",
  ]
}