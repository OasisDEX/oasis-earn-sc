{
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@typechain/*": [
        "../dma-contracts/typechain/*"
      ],
      "@dma-library/strategies": [
        "./src/strategies/index.ts"
      ],
      "@dma-library/types": [
        "./src/types/index.ts"
      ],
      "@dma-library/actions/action-factory": [
        "./src/actions/action-factory.ts"
      ],
      "@dma-library/*": [
        "./src/*"
      ],
      "@dma-common/constants": ["../dma-common/constants"],
      "@dma-common/utils/*": ["../dma-common/utils/*"],
      "@dma-common/types/*": ["../dma-common/types/*"],
      "@dma-common/test-utils": ["../dma-common/test-utils/index.ts"],
      "@dma-common/test-utils/*": ["../dma-common/test-utils/*"],
      "@dma-deployments/utils/*": ["../dma-deployments/utils/*"],
      "@dma-deployments/types/*": ["../dma-deployments/types/*"],
      "@dma-deployments/constants": ["../dma-deployments/constants/index.ts"]
    },
    "types": [
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": ["node_modules", "../dma-deployments", "../dma-common"]
}