  "license": "ISC",
  "engines": {
    "node": ">=14.17"
  },
  "private": "true",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "deploy": "npx hardhat run",
    "test": "npx hardhat test && yarn workspaces run test",
    "test:gas": "USE_GAS_ESTIMATES=1 yarn test",
    "test:1inch": "RUN_1INCH_TESTS=1 yarn test",
    "test:1inch:gas": "RUN_1INCH_TESTS=1 yarn test:gas",
    "lint": "eslint . --ext .ts --max-warnings=0 && prettier --list-different 'contracts/**/*.sol'",
    "lint:fix": "eslint . --fix --ext .ts",
    "format": "prettier --check \"./**/*.{ts,tsx,js,jsx}\"",
    "format:fix": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "prettier:sol": "prettier --write 'contracts/**/*.sol'",
    "compile": "npx hardhat compile",
    "start": "npx hardhat node --network hardhat",
    "deploy:fallback": "yarn compile && npx hardhat deploy --usefallbackswap --network local --debug",
    "deploy:1inch": "yarn compile && npx hardhat deploy --network local --debug",
    "steth": "npx hardhat createPosition --network local",
    "steth:fallback": "npx hardhat createPosition --dummyswap --network local",
    "close": "npx hardhat closePosition --network local",
    "close:fallback": "npx hardhat closePosition --dummyswap --network local"
  },
